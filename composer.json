{
    "name": "qingpizi/hyperf-framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "require": {
        "php": ">=7.4",
        "ext-openssl": "*",
        "hyperf/amqp": "^2.2",
        "hyperf/cache": "^2.2",
        "hyperf/command": "^2.2",
        "hyperf/config": "^2.2",
        "hyperf/constants": "^2.2",
        "hyperf/database": "^2.2",
        "hyperf/db-connection": "^2.2",
        "hyperf/framework": "^2.2",
        "hyperf/guzzle": "^2.2",
        "hyperf/http-server": "^2.2",
        "hyperf/logger": "^2.2",
        "hyperf/memory": "^2.2",
        "hyperf/model-cache": "^2.2",
        "hyperf/paginator": "^2.2",
        "hyperf/process": "^2.2",
        "hyperf/rate-limit": "^2.2",
        "hyperf/redis": "^2.2",
        "hyperf/signal": "^2.2",
        "hyperf/validation": "^2.2",
        "symfony/serializer": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~3.1.0",
        "hyperf/devtool": "^2.2",
        "mockery/mockery": "~1.4.4",
        "phpstan/phpstan": "~0.12.99",
        "phpunit/phpunit": "~9.5.10",
        "swoole/ide-helper": "~4.7.1",
        "symfony/var-dumper": "~5.3.8"
    },
    "autoload": {
        "files": [
            "src/Kernel/Functions.php"
        ],
        "psr-4": {
            "Qingpizi\\HyperfFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Qingpizi\\HyperfFramework\\ConfigProvider"
        }
    }
}
