<?php

declare(strict_types=1);

namespace App\Controller\%GROUP_NAME%;

use App\Logic\%GROUP_NAME%\StoreLogic;
use App\Request\%GROUP_NAME%\StoreRequest;
use Qingpizi\HyperfFramework\Controller\BaseController;

class StoreController extends BaseController
{
    public function handle(StoreRequest $request)
    {
        $validated = $request->validated();
        $logic = $this->container->get(StoreLogic::class);
        $data = $logic->handle($validated);
        return $this->response->success($data);
    }
}
